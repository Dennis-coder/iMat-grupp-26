<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.text.Font?>
<fx:root xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" type="AnchorPane">
   <children>
      <Label fx:id="label" onMouseClicked="#onClick" onMouseEntered="#onHover" onMouseExited="#onHoverStopped" text="Label">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</fx:root>
